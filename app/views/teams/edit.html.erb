<% provide(:title, "Edit team") %>
<h1>Change your passphrase</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
		<%= form_for(@team) do |f| %>
			<%= render 'shared/error_messages' %>
	
			<%= f.label "Passphrase"  %>
  		<%= f.password_field :passphrase, class: 'form-control' %>

  		<%= f.submit "Save", class: "btn btn-primary" %>
		<% end %>
  </div>
</div>
